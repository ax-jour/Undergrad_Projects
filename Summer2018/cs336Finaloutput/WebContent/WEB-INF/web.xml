  <welcome-file-list>
    <welcome-file>HomePage.jsp</welcome-file>
  </welcome-file-list>
  
   <jsp-config>
 
  <taglib>
    <taglib-uri>http://java.sun.com/jstl/fmt</taglib-uri>
